/*tipo de selector {
  propiedad:valor;
  propiedad-2:valor;
}*/

/*Selector universal*/
* {
  border: 1px solid red;
  maring: 10px;
}
/*Selector de etiqueta*/
header {
  background: blue;
}

section {
  background: purple;
}
/*Selector descendente*/
article h3  {
  color: green;
}

/*Selectores de clase*/
.tittle-a {
  background: yellow;
}
.tittle-b {
  background: salmon;
}

/*selector de id*/
#header {
  border: 3px solid black;
}
#guitarras {
  border: 3px solid brown;
}
#portada {
  border: 3px solid orange;
}
